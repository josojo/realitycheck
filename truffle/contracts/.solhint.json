diud{                                              
    "extends": "default",                      
    "rules": {                                 
        "indent": ["error", 4],                
        "quotes": ["error", "double"],         
        "max-line-length": ["error", 129],
        "function-max-lines": ["error", 45],
        "max-states-count": ["error", 18]
    }                     
}